{
  "expo": {
    "name": "nextup_manager",
    "slug": "nextup_manager",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nextup"
    },
    "description": "Build for the first testing suit.",
    "githubUrl": "https://github.com/dkennedy881/nextup_manager"
  }
}
